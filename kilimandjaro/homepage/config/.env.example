# Homepage Environment Variables
# Copy this file to .env and fill in your actual API keys and credentials
# These variables are referenced in services.yaml using {{HOMEPAGE_VAR_*}} syntax

# Immich API Key
# Generate from: Immich Settings > API Keys
HOMEPAGE_VAR_IMMICH_API_KEY=your_immich_api_key_here

# Nextcloud Credentials
# Use an app password if 2FA is enabled
HOMEPAGE_VAR_NEXTCLOUD_USERNAME=your_nextcloud_username
HOMEPAGE_VAR_NEXTCLOUD_PASSWORD=your_nextcloud_password_or_app_password

# Portainer API Key
# Generate from: Portainer Settings > Users > Access tokens
HOMEPAGE_VAR_PORTAINER_API_KEY=your_portainer_api_key_here

# AdGuard Home Credentials
HOMEPAGE_VAR_ADGUARD_USERNAME=your_adguard_username
HOMEPAGE_VAR_ADGUARD_PASSWORD=your_adguard_password

# Authentik API Key
# Generate from: Authentik Admin > Tokens & App passwords
HOMEPAGE_VAR_AUTHENTIK_API_KEY=your_authentik_api_key_here
